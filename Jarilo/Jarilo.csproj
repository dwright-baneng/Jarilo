<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <PackageId>Jarilo</PackageId>
    <PackageVersion>1.1.0-beta</PackageVersion>
    <Authors>Karol Hinz</Authors>
    <Description>.NET Core 2.0 command line application framework.</Description>
    <ProjectUrl>https://github.com/ninjah187/Jarilo</ProjectUrl>
    <PackageTags>command line interface cli app terminal console</PackageTags>
